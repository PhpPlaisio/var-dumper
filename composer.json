{
  "name": "plaisio/var-dumper",
  "description": "PhpPlaisio: Var Dumper",
  "keywords": [
    "PhpPlaisio",
    "Var",
    "Dumper"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "plaisio/var-dumper-writer": "^1.1",
    "setbased/exception": "^2.1"
  },
  "require-dev": {
    "ext-mbstring": "*",
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Debug\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Debug\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
