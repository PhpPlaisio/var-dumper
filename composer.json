{
  "name": "plaisio/var-dumper",
  "description": "PhpPlaisio: Var Dumper",
  "keywords": [
    "PhpPlaisio",
    "Var",
    "Dumper"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "plaisio/var-dumper-writer": "^1.0.0",
    "setbased/exception": "^2.0.0"
  },
  "require-dev": {
    "phing/phing": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Debug\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Debug\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
