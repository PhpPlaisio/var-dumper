{
  "name": "plaisio/var-dumper",
  "description": "PhpPlaisio: Var Dumper",
  "keywords": [
    "PhpPlaisio",
    "Var",
    "Dumper"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "plaisio/var-dumper-writer": "^3.0.0",
    "setbased/exception": "^2.3.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "ext-mbstring": "*",
    "phing/phing": "^3.0.0-RC4",
    "phpunit/phpunit": "^9.6.3"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Debug\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Debug\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
